    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "ts-standard": "^12.0.1",
    "tsd": "^0.31.0",
    "typescript": "^5.2.2",
    "walker": "^1.0.8"
  },
  "tsd": {
    "directory": "test"
  }
}
