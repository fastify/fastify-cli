{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Launch Fastify",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "dev:start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "preLaunchTask": "npm: watch:ts"
        }
    ]
}
